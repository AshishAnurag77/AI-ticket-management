{
  "name": "ai-ticket-system",
  "version": "1.0.0",
  "description": "AI-Powered Ticket Management System",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\" --kill-others-on-fail",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:inngest": "cd backend && npm run inngest-dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && node index.js",
    "frontend:start": "cd frontend && npm run dev"
  },
  "keywords": ["ai", "ticket", "management", "inngest", "react"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}